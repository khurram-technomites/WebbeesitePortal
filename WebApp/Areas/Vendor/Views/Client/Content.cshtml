<style>
    .Selected {
    }
    /**/
    .ck.ck-editor {
        position: relative;
        z-index: 0;
        width: 100%;
    }

    font-size-11 {
        font-size: 11px !important;
    }

    custom-bg-input {
        background-color: #f6f5f5;
    }
    /*#tns1-mw {
        width: 83% !important
    }*/

    checkbox label:after {
        content: '';
        display: table;
        clear: both;
    }

    .checkbox .cr {
        position: relative;
        display: inline-block;
        border: 1px solid #a9a9a9;
        border-radius: .25em;
        width: 1.3em;
        height: 1.3em;
        float: left;
        margin-right: .5em;
    }

        .checkbox .cr .cr-icon {
            position: absolute;
            font-size: .8em;
            line-height: 0;
            top: 50%;
            left: 15%;
        }

    .checkbox label input[type="checkbox"] {
        display: none;
    }

        .checkbox label input[type="checkbox"] + .cr > .cr-icon {
            opacity: 0;
        }

        .checkbox label input[type="checkbox"]:checked + .cr > .cr-icon {
            opacity: 1;
        }

        .checkbox label input[type="checkbox"]:disabled + .cr {
            opacity: .5;
        }
</style>

<div class="content d-flex flex-column flex-column-fluid" id="kt_content">
    <div class="d-flex flex-column-fluid">
        <div class="container">
            <div class="row">
                <div class="col-xl-12">
                    <div class="card card-custom gutter-b">
                        <div class="card-body">

                            <div class="card-body pr-0 pl-0 pt-4 pb-0" id="formInputs">
                                <div class="form-group row">
                                    <input id="ClientId" value="@ViewBag.ClientId" hidden />
                                    <div class="col-lg-12">
                                        <div class="row">
                                            <div class="col-md-12 border-right-lg">
                                                <div class="card-title"><h5>Content</h5></div>
                                                <div class="card-title mb-1"><h5>Does the client need a new domain?</h5></div>
                                                <div class="row pl-4 mb-7">
                                                    <div class="d-flex justify-content-left align-items-center">
                                                        <div class="form-check">
                                                            <input class="form-check-input" @(Model.IsDomainRequired == true ? "checked" : "")   type="checkbox" value="" id="YesNewDomain">
                                                            <span class="font-size-xs">Yes, needs a new domain.</span>
                                                        </div>
                                                        <div class="form-check ml-4">
                                                            <input class="form-check-input" @(Model.IsDomainRequired == false ? "checked" : "")   type="checkbox" value="" id="AlreadyDomain">
                                                            <span class="font-size-xs">No, already has a domain.</span>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="row">
                                                    <div class="col-sm-6 mb-5 newdomainsection ">
                                                        <div class="form-group mb-3 ">
                                                            <label class="control-label" style="font-size:11px;">Domain</label>
                                                            <div class="input-group">
                                                                <input value="" type="text" id="Domain" class="form-control branch-input bg-dark-o-15 border-0" required placeholder="Type Here" />
                                                                <div class="input-group-append my-cursor btndomain"><span class="input-group-text bg-warning rounded text-light">Add</span></div>
                                                            </div>
                                                        </div>
                                                        <div class="w-100">
                                                            <div class="row pl-4 pr-4 domainsection">
                                                                @foreach (var domain in Model.ClientDomainSuggestionsViewModels)
                                                                {
                                                                    <div class="m-1 p-0">
                                                                        <div class="bg-gray-200 d-flex justify-content-center align-items-center rounded p-1">
                                                                            <span class="font-size-xs font-weight-boldest">@domain.Domain</span>
                                                                            <span><i class="fa fa-trash text-red font-size-sm ml-3  my-cursor btndeletedomain" data-id="@domain.Id"></i></span>
                                                                        </div>
                                                                    </div>
                                                                }

                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-6 mb-5 AlreadyDomainSection" hidden>
                                                        <div class="row">
                                                            <div class="col-6 mb-5">
                                                                <div class="form-group mb-3 ">
                                                                    <label class="control-label" style="font-size:11px;">Website Address</label>
