                                                                        <input name="Website" value="@Model.Website" type="text" id="Domain" class="form-control branch-input bg-dark-o-15 border-0" required placeholder="Type Here" />
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="col-6 mb-5">
