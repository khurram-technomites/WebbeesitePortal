<script>
    function GenerateInvoice(element, Id) {
        $(element).addClass('spinner spinner-sm spinner-left').attr('disabled', true);
        debugger
        $.ajax({
            url: "/Vendor/Client/Finalize",
            type: "POST",
            data: { 'PurchaseId': Id },
            success: function(response) {
                if (response.success) {
                    $(element).removeClass('spinner spinner-sm spinner-left').attr('disabled', false);
                    if (response.url.Data != undefined) {
                        window.open(response.url.Data, '_blank');
                    }
                    else {
                        if (response.url.Status != "Failed") {
                            window.open(response.url, '_blank');
                        }
                        else {
                            toastr.error("An error Occured Bad Request");
                            return false;
                        }

                    }

                }
                else {
                    toastr.error(response.message);
                    return false;
                }
            },
            error: function(er) {
                toastr.error(er);
            }
        });

    }
</script>
