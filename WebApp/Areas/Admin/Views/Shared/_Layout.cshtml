    <link rel="icon" href="~/favicon.png">
    <style>
        .max-h-40px {
            max-height: 60px !important;
        }

        .text-muted {
            color: #000000 !important;
        }

        i {
            color: #000000;
        }

        .spinner-modal {
            margin-top: 3.75rem !important;
        }

        hr {
            width: 100%;
            text-align: left;
