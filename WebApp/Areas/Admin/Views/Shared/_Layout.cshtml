                                            <span class="symbol-label text-white font-size-h5 font-weight-bold bg-white-o-30">@User.Identity.Name.Split("@")[0].Remove(2)</span>
                                        </span>
                                    </div>
                                </div>
                                <!--end::Toggle-->
                            </div>
                            <!--end::User-->
                        </div>
                        <!--end::Topbar-->
                    </div>
                    <!--end::Container-->
                </div>
                <!--end::Header-->
                @RenderBody()
