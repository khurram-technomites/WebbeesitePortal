        function GenerateInvoice(Id) {
            $.ajax({
                url: "/Vendor/Client/Finalize",
                type: "POST",
                data: { 'PurchaseId': Id },
                success: function (response) {
                    if (response.success) {
                        window.open(response.url, '_blank');
                    }
                    else {
                        toastr.error(response.message);
                        return false;
                    }
                },
                error: function (er) {
                    toastr.error(er);
                }
            });

        }
