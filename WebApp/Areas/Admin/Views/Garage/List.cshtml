@model IEnumerable<WebApp.ViewModels.GarageViewModel>

@{
    ViewBag.Title = "List";
    Layout = null;
}
<style>
    #kt_datatable1 tbody tr td:last-child {
        white-space: nowrap;
    }
</style>

<link href="~/Admin/plugins/custom/datatables/datatables.bundle.css" rel="stylesheet" type="text/css" />
<div  id="kt_content">
    <div class="d-flex flex-column-fluid">
        <div class="container">
            <div class="row">
                <div class="col-xl-12">
                    <div class="card card-custom gutter-b">
                        <div class="card-header flex-wrap border-0 pt-6 pb-0">
                            <div class="card-title">
                                <h3 class="card-label">
                                    Clients
                                    <span class="d-block text-muted pt-2 font-size-sm">Manage Clients Here...</span>
                                </h3>
                            </div>
                        </div>
                        <div class="card-body">
                            <table class="table table-separate table-head-custom table-checkable" id="kt_datatable1">
                                <thead>
                                    <tr>
                                        <th>Date</th>
                                        <th>Name(Trade License)</th>
                                        @*<th style="width:180px;">Name</th>*@
                                        <th>Contact</th>
                                        <th>Status</th>
                                        <th>Actions</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>
                                                @(item.CreationDate.ToString("dd MMM yyyy, hh:mm tt"))
                                            </td>
                                            <td>
                                                @(item.Logo + "|" + item.NameAsPerTradeLicense + "|" + item.ContactPersonEmail)
                                                @*@item.ContactPersonName
                                                    <span style="font-size:12px;"><a href="javascript:void(0);">@item.ContactPersonEmail</a></span>*@
                                            </td>
                                            <td> @item.ContactPersonNumber</td>
                                            <td>
                                                @*<label class="label label-inline label-primary">@item.Status</label>*@
                                                @item.Status
                                            </td>
                                            <td nowrap="nowrap">@item.Id</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="myModal" class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display: none;">
    <div class="modal-dialog modal-dialog-centered modal-xl">
        <div class="modal-content" style="min-height:100px">
            <div id='myModalContent'>
                <div class="row justify-content-center">
                    <div class="col-16">
                        <div class="kt-spinner kt-spinner--lg kt-spinner--dark kt-spinner-modal"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
