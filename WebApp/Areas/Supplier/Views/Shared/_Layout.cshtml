                    //} else {
                    if (text.includes('AED')) {
                        text = text.replace('AED', '').trim();
                        $(v).html('AED ' + numeral(text).format('0,0.00'));
                    } else {
                        $(v).html(numeral(text).format('0,0.00'));
                    }
                    //}
                    $(v).addClass('formatted');
                }
            });
